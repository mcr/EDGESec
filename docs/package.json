{
  "name": "cropdoc-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "serve-build": "node scripts/serve-build.js",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "pdf": "docusaurus start --no-open --port=3002 & pid=\"$!\" && trap 'echo \"killing $pid\" && kill -2 \"$pid\"' EXIT && sleep 5 && docusaurus-pdf http://localhost:3002/ManySecured/",
    "lint": "eslint *.js scripts/**/*.js src/"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.56",
    "@docusaurus/preset-classic": "^2.0.0-alpha.56",
    "docusaurus-lunr-search": "^1.0.3",
    "docusaurus-pdf": "^1.1.1",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remark-mermaid-dataurl": "^1.0.0",
    "yargs": "^15.3.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@nqminds/eslint-config": "^0.1.5",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{md,yaml,yml,json,css,mdx}": "prettier --write"
  }
}
